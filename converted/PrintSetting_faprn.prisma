model PrintSetting {
  @@map("faprn")
  
  printSettingId String @id(map: "pkfaprn") @map("keyfaprn") @db.Char(15)
  transactionTypeId String? @map("keyrevrt") @db.Char(15)
  companyId String? @map("keycmcmp") @db.Char(15)
  billingMethodId String @map("keycmdbm") @db.Char(15)
  reportLayoutId String? @map("keycmrpl") @db.Char(15)
  invoiceTemplateId String? @map("keycmfrx") @db.Char(15)
  requirementScenarioLevelId String? @map("keykrkrv") @db.Char(15)
  integrationSchemaId String? @map("keycmmap") @db.Char(15)
  type Int? // TODO: Confirm the context of 'type'
  ocrQuantity Int @map("ocrqty") @db.TinyInt
  defaultBillingMethod Int @map("defaultdbm") @db.TinyInt
  eInvoiceId String? @map("einvoiceid") @db.Char(4)
  eInvoiceId2 String? @map("einvid2") @db.Char(4)
  exportId Int? @map("exportid") 
  noSkip Int @map("noskip") @db.TinyInt
  classId Int? @map("classid")
  invoicePrinciple Int? @map("remkrfkh") @db.TinyInt
  noInterrupt Int @map("nointerrpt") @db.TinyInt
  invoiceHeaderOverride Int @map("fadbhoverr") @db.TinyInt
  b2BExportKind Int @default(0, map: "DF__faprn__b2bexpkin__76124D36") @map("b2bexpkind") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Verify usage of timestamp

  // Relations
  company Company? @relation(fields: [companyId], references: [companyId], onDelete: NoAction, onUpdate: NoAction, map: "fkfaprnkeycmcmp")
  integrationSchema IntegrationSchema? @relation(fields: [integrationSchemaId], references: [integrationSchemaId], onUpdate: NoAction, map: "fkfaprnkeycmmap")
  reportLayout ReportLayout? @relation(fields: [reportLayoutId], references: [reportLayoutId], onUpdate: NoAction, map: "fkfaprnkeycmrpl")
  requirementScenarioLevel RequirementScenarioLevel? @relation(fields: [requirementScenarioLevelId], references: [requirementScenarioLevelId], onDelete: NoAction, onUpdate: NoAction, map: "fkfaprnkeykrkrv")
  transactionType TransactionType? @relation(fields: [transactionTypeId], references: [transactionTypeId], onDelete: NoAction, onUpdate: NoAction, map: "fkfaprnkeyrevrt")

  @@index([companyId], map: "fkfaprncmcmp")
  @@index([billingMethodId], map: "fkfaprncmdbm")
  @@index([invoiceTemplateId], map: "fkfaprncmfrx")
  @@index([integrationSchemaId], map: "fkfaprncmmap")
  @@index([reportLayoutId], map: "fkfaprncmrpl")
  @@index([requirementScenarioLevelId], map: "fkfaprnkrkrv")
  @@index([transactionTypeId], map: "fkfaprnrevrt")
}