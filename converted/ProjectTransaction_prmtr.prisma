model ProjectTransaction {
  @@map("prmtr")

  projectTransactionId String @id(map: "pkprmtr") @map("keyprmtr") @db.Char(15)
  projectId String @map("keyprprj") @db.Char(15) // Referring to Project
  subProjectId String? @map("keyprppr") @db.Char(15) // Referring to SubProject
  registeredById String? @map("keysyusr") @db.Char(15) // Referring to User
  resourceId String? @map("keycmctc") @db.Char(15) // Referring to Contact/Resource
  supplierContactId String? @map("keycmctc2") @db.Char(15) // Referring to Contact/Supplier
  articleId String? @map("keycmart") @db.Char(15) // Referring to Article
  storageLocationId String? @map("keyimial") @db.Char(15) // Potential mapping based on context
  supplierInvoiceId String? @map("keylrfkh") @db.Char(15) // Referring to SupplierInvoice
  ledgerRowId String? @map("keyrevrr") @db.Char(15) // Referring to LedgerRow
  actionGroupLevel1Id String? @map("keyskafb") @db.Char(15) // Referring to ActionGroupLevel1
  isSupplierInvoice Int @default(0, map: "DF__prmtr__lrinvoice__38179A75") @map("lrinvoice") @db.TinyInt
  quantityAmount Float @default(0, map: "DF__prmtr__quantity__390BBEAE") @map("quantity") @db.Money
  debitedQuantityAmount Float @default(0, map: "DF__prmtr__debqty__39FFE2E7") @map("debqty") @db.Money
  totalAmount Float @default(0, map: "DF__prmtr__amount__3AF40720") @map("amount") @db.Money
  debitedAmount Float @default(0, map: "DF__prmtr__debamount__3BE82B59") @map("debamount") @db.Money
  markupPercentage Float @default(0, map: "DF__prmtr__addprcnt__3CDC4F92") @map("addprcnt") @db.Money
  description String @map("caption") @db.VarChar(70)
  startDate DateTime @map("fdate") @db.DateTime
  endDate DateTime? @map("tdate") @db.DateTime
  startTime Int? @map("ftime") @db.SmallInt
  endTime Int? @map("ttime") @db.SmallInt
  isDebited Int @default(0, map: "DF__prmtr__debit__3DD073CB") @map("debit") @db.TinyInt
  expenseGroup Int? @map("expgrp") @db.SmallInt
  certificationStatus Int @default(0, map: "DF__prmtr__atstatus__3EC49804") @map("atstatus") @db.TinyInt
  sourceType Int @default(1, map: "DF__prmtr__source__3FB8BC3D") @map("source") @db.TinyInt
  exportDate DateTime? @map("exportdate") @db.DateTime
  isBlocked Int @default(0, map: "DF__prmtr__blocked__40ACE076") @map("blocked") @db.TinyInt
  cleanedInvoice Int @default(0, map: "DF__prmtr__invrem__41A104AF") @map("invrem") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Determine the purpose of timestamp for future passes

  pract ActivityTime[] // Updated to ActivityTime based on forward pass
  article Article? @relation(fields: [keycmart], references: [articleId], onDelete: NoAction, onUpdate: NoAction, map: "fkprmtrkeycmart")
  resource Contact? @relation("prmtr_keycmctcTocmctc", fields: [keycmctc], references: [keycmctc], onDelete: NoAction, onUpdate: NoAction, map: "fkprmtrkeycmctc")
  supplierContact Contact? @relation("prmtr_keycmctc2Tocmctc", fields: [keycmctc2], references: [keycmctc], onDelete: NoAction, onUpdate: NoAction, map: "fkprmtrkeycmctc2")
  supplierInvoice SupplierInvoice? @relation(fields: [keylrfkh], references: [supplierInvoiceId], onDelete: NoAction, onUpdate: NoAction, map: "fkprmtrkeylrfkh")
  subProject SubProject? @relation(fields: [keyprppr], references: [subProjectId], onDelete: NoAction, onUpdate: NoAction, map: "fkprmtrkeyprppr")
  project Project @relation(fields: [keyprprj], references: [projectId], onUpdate: NoAction, map: "fkprmtrkeyprprj")
  ledgerRow LedgerRow? @relation(fields: [keyrevrr], references: [ledgerRowId], onDelete: NoAction, onUpdate: NoAction, map: "fkprmtrkeyrevrr")
  actionGroupLevel1 ActionGroupLevel1? @relation(fields: [keyskafb], references: [id], onUpdate: NoAction, map: "fkprmtrkeyskafb")

  @@index([keycmart], map: "fkprmtrcmart")
  @@index([keycmctc], map: "fkprmtrcmctc")
  @@index([keycmctc2], map: "fkprmtrcmctc2")
  @@index([keylrfkh], map: "fkprmtrlrfkh")
  @@index([keyprppr], map: "fkprmtrprppr")
  @@index([keyprprj], map: "fkprmtrprprj")
  @@index([keyrevrr], map: "fkprmtrrevrr")
  @@index([keyskafb], map: "fkprmtrskafb")
  @@index([keysyusr], map: "fkprmtrsyusr")
  @@index([keycmctc, startDate, keyprprj], map: "inprmtr_2LU0GRMG7")
}