model Selection {
  @@map("cmqry")

  selectionId String @id(map: "pkcmqry") @map("keycmqry") @db.Char(15)
  userId String? @map("keysyusr") @db.Char(15)
  name String @map("caption") @db.VarChar(250)
  isInternal Int @default(0, map: "DF__cmqry__internal__6F3F55D2") @map("internal") @db.TinyInt
  accessRead Int @default(0, map: "DF__cmqry__accr__70337A0B") @map("accr") @db.TinyInt
  accessWrite Int @default(0, map: "DF__cmqry__accw__71279E44") @map("accw") @db.TinyInt
  query String @map("qryquery") @db.VarChar(Max)
  description String? @map("qrymemo") @db.VarChar(Max)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  dataSource DataSource[] @relation("fkbsendkeycmqry") // Updated from bsend to new model name DataSource
  batchProcessRow BatchProcessRow[] @relation("fkcmbtrkeycmqry")
  selectionRow SelectionRow[] @relation("fkcmqrrkeycmqry")
  reportBatchRow ReportBatchRow[] @relation("fkcmrbrkeycmqry") // Updated from cmrbr to new model name ReportBatchRow
  reportColumn ReportColumn[] @relation("fkcmrdckeycmqry")
  reportRow ReportRow[] @relation("fkcmrdrkeycmqry")
  favorite Favorite[] @relation("fkcmunfkeycmqry") // Updated from cmunf to new model name Favorite

  @@index([userId], map: "fkcmqrysyusr")
}