model Report {
  @@map("cmrpt")

  reportId String @id(map: "pkcmrpt") @map("keycmrpt") @db.Char(15)
  classId String? @map("keysycls") @db.Char(15) // TODO: Verify context or new name when available
  textClusterId2 String? @map("keysytxc2") @db.Char(15) // TODO: Verify context or new name when available
  textClusterId String? @map("keysytxc") @db.Char(15) // TODO: Verify context or new name when available
  imageClusterId String @map("keysyimc") @db.Char(15) // TODO: Converting to English equivalent context
  userId String? @map("keysyusr") @db.Char(15) // TODO: Verify context or new name when available
  reportCaption String @map("caption") @db.VarChar(250)
  reportClass String? @map("class") @db.VarChar(100)
  reportDestination Int @default(0, map: "DF__cmrpt__rptdest__09B922A4") @map("rptdest")
  userDefined Int @default(0, map: "DF__cmrpt__userdef__0AAD46DD") @map("userdef") @db.TinyInt
  readAccess Int @default(0, map: "DF__cmrpt__accr__0BA16B16") @map("accr") @db.TinyInt
  writeAccess Int @default(0, map: "DF__cmrpt__accw__0C958F4F") @map("accw") @db.TinyInt
  rowTemplate Int @default(0, map: "DF__cmrpt__rptrsh__0D89B388") @map("rptrsh") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Revisit when more context is known

  formTemplates FormTemplate[] @relation("fkbsdefkeycmrpt")
  reportBatchRows ReportBatchRow[]
  logEvents LogEvent[]
  digitalSignatures DigitalSignature[]
  //@TODO: Fix or remove index if nothing to index
  @@index([classId], map: "fkcmrptsycls") 
  @@index([imageClusterId], map: "fkcmrptsyimc") 
  @@index([textClusterId], map: "fkcmrptsytxc") 
  @@index([textClusterId2], map: "fkcmrptsytxc2") 
  @@index([userId], map: "fkcmrptsyusr") 
}