model LedgerRow {
  @@map("revrr")

  ledgerRowId String @id(map: "pkrevrr") @map("keyrevrr") @db.Char(15)
  verificationId String @map("keyrevrh") @db.Char(15)
  codeTypeId String? @map("keyrekpy") @db.Char(15)
  unitId String? @map("keycmuni") @db.Char(15)  // Referring to Unit
  currencyId String? @map("keycmcuh") @db.Char(15) // Referring to Currency
  rowText String @map("text") @db.Char(30)
  accountingYear Int @map("year") @db.SmallInt
  accountingPeriod Int @map("period") @db.TinyInt
  volumeAmount Float @map("volume") @db.Money
  unitDescription String? @map("unit") @db.Char(10)
  modificationBlocked Int @map("omf") @db.TinyInt
  propertyVat Int @map("vattype") @default(0, map: "DF__revrr__vattype__68FAC8F2") @db.TinyInt
  debitAmount Float @map("db") @db.Money
  creditAmount Float @map("cr") @db.Money
  autoPostingStatus Int @map("status") @db.TinyInt
  certificationStatus Int @map("atstatus") @default(0, map: "DF__revrr__atstatus__69EEED2B") @db.TinyInt
  cancellationFlag Int @map("delflag") @db.TinyInt
  p1 String @map("p1") @db.Char(10) // TODO: Determine use of p1 field
  p2 String @map("p2") @db.Char(10) // TODO: Determine use of p2 field
  p3 String @map("p3") @db.Char(10) // TODO: Determine use of p3 field
  p4 String @map("p4") @db.Char(10) // TODO: Determine use of p4 field
  p5 String @map("p5") @db.Char(10) // TODO: Determine use of p5 field
  p6 String @map("p6") @db.Char(10) // TODO: Determine use of p6 field
  p7 String @map("p7") @db.Char(10) // TODO: Determine use of p7 field
  p8 String @map("p8") @db.Char(10) // TODO: Determine use of p8 field
  p9 String @map("p9") @db.Char(10) // TODO: Determine use of p9 field
  p10 String @map("p10") @db.Char(10) // TODO: Determine use of p10 field
  timestamp String @map("timestamp") @db.Char(10) // TODO: Determine the purpose of timestamp 

  projectTransaction ProjectTransaction[] @relation("fkprmtrkeyrevrr")
  preSystemLogs PreSystemLog[] @relation("fkrefslkeyrevrr")

  currency Currency? @relation(fields: [keycmcuh], references: [keycmcuh], onDelete: NoAction, onUpdate: NoAction, map: "fkrevrrkeycmcuh ")
  unit Unit? @relation(fields: [keycmuni], references: [keycmuni], onDelete: NoAction, onUpdate: NoAction, map: "fkrevrrkeycmuni ")
  codeType CodeType? @relation(fields: [keyrekpy], references: [keyrekpy], onDelete: NoAction, onUpdate: NoAction, map: "fkrevrrkeyrekpy")
  verification Verification @relation(fields: [keyrevrh], references: [keyrevrh], onDelete: Cascade, onUpdate: NoAction, map: "fkrevrrkeyrevrh")

  @@index([keyrekpy], map: "fkrevrrrekpy")
  @@index([keyrevrh], map: "fkrevrrrevrh")
  @@index([accountingYear, accountingPeriod, p1, p2, p3, p4, p5, p6, p7, p8, p9, p10], map: "inrevrr_23D0SGMH6")
  @@index([keyrevrr, keyrevrh, cancellationFlag, debitAmount], map: "inrevrr_23D0SGMXO")
  @@index([cancellationFlag, p1, p2, p3, p4, p5, keyrevrh], map: "inrevrr_23D0SGNEL")
  @@index([keyrevrh, keyrevrr, p2, cancellationFlag], map: "inrevrr_momsrapport")
  @@index([p1], map: "inrevrr_p1")
}