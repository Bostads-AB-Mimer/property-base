model OrderClass {
  @@map("nybks")

  orderClassId String @id(map: "pknybks") @map("keynybks") @db.Char(15)
  orderCode String @unique(map: "aknybks") @map("code") @db.VarChar(10)
  name String? @map("caption") @db.VarChar(30)
  timestamp String @map("timestamp") @db.Char(10) // TODO: Evaluate the use of timestamp later

  nycty CylinderType[] @relation("fknyntykeynybks") // Updated relation to CylinderType using the new model name
}