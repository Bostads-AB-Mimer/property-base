model ObjectCategory {
  @@map("alkat")

  // Fields
  id String @id(map: "pkalkat") @map("keyalkat") @db.Char(15)
  // Object category code, similar to Objektskategori
  objectCategoryCode String @map("code") @db.VarChar(30)
  // Name of the category
  name String @map("caption") @db.VarChar(30)
  // Tax percentage rate
  taxPercentage Decimal? @map("taxprcnt") @db.Decimal(15, 10)
  // Tax calculation flag
  taxCalculationFlag Int @default(0, map: "DF__alkat__taxcalc__2DEAFAED") @map("taxcalc") @db.TinyInt
  // Indicates if included in summarized values
  includedInSummary Int @default(0, map: "DF__alkat__incsum__2EDF1F26") @map("incsum") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  // Relations
  // TODO: Make sure the relation name matches the actual one used in the Asset model
  // assets Asset[] @relation("fkalobjkeyalkat")
}