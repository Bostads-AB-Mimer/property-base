model LeaseApprovalSetting {
  @@map("hystg")

  id String @id(map: "pkhystg") @map("keyhystg") @db.Char(15)
  contractId String @map("keyhyobj") @db.Char(15)
  approvalDate DateTime @map("createdate") @db.DateTime
  batchApprovalDate DateTime? @map("applydate") @db.DateTime
  settingDescription String @map("setting") @db.VarChar(2500)
  type Int @map("type") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Revisit usage of timestamp for potential renaming

  // TODO: 'hyobj' model not defined in the new schema, relation commented out
  // hyobj hyobj @relation(fields: [contractId], references: [keyhyobj], onDelete: Cascade, onUpdate: NoAction, map: "fkhystgkeyhyobj")
  
  @@index([contractId], map: "fkhystghyobj")
}