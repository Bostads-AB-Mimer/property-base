model TransactionType {
  @@map("revrt")

  transactionTypeId String @id(map: "pkrevrt") @map("keyrevrt") @db.Char(15)
  moduleId String @map("keysymdl") @db.Char(15) // TODO: change moduleId when knowing more about its usage/relationship
  routineId String? @map("keycmrtn") @db.Char(15) // TODO: Confirm and update once Routine relation is clearer
  routeId String? @map("keyatway") @db.Char(15) 
  typeId Int @unique(map: "akrevrt1") @map("typeid") @db.SmallInt 
  name String? @map("name") @db.VarChar(30)
  forceControl Int @map("forcectrl") @db.TinyInt
  setVerifyId Int @map("setverid") @db.SmallInt
  connectionControl Int @map("connctrl") @db.TinyInt
  accountControl Int @map("accctrl") @db.TinyInt
  accountControl2 Int @map("accctrl2") @db.TinyInt
  debitTypeQuantity Int @map("fadbtqty") @db.SmallInt
  directUpdateAccount Int @map("dirupdacc") @db.TinyInt
  bundleVoucherRow Int @default(0, map: "DF__revrt__bundlevrr__43144BD5") @map("bundlevrr") @db.TinyInt
  isActive Int @map("vrtused") @db.TinyInt 
  systemStandard Int @map("repabkod") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Clarify usage and rename if needed

  printSettings PrintSetting[] @relation("fkfaprnkeyrevrt")
  autogiroTransactionTypeCustomers AutogiroTransactionTypeCustomer[] @relation("fkkragkkeyrevrt")
  amortizationPlans AmortizationPlan[] @relation("fkkrampkeyrevrt")
  invoices Invoice[] @relation("fkkrfkhkeyrevrt")
  preSystemLogs PreSystemLog[] @relation("fkrefslkeyrevrt")

  // Relations
  routine Routine? @relation(fields: [routineId], references: [keycmrtn], onDelete: NoAction, onUpdate: NoAction, map: "fkrevrtkeycmrtn")

  @@index([routineId], map: "fkrevrtcmrtn")
  @@index([moduleId], map: "fkrevrtsymdl")
  @@index([transactionTypeId, name, typeId], map: "inrevrt_2CL0W1RQI")
}