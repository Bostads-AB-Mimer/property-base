model AutogiroTransaction {
  @@map("kragl")

  autogiroTransactionId String @id(map: "pkkragl") @map("keykragl") @db.Char(15)
  userId String @map("keysyusr") @db.Char(15) // Based on keysyusr which refers to Användare or User
  contactId String? @map("keycmctc") @db.Char(15) // Refers to Contact as per previous conversions
  invoiceId String? @map("keykrfkh") @db.Char(15) // Based on reference to Faktura, Swedish for Invoice
  transactionId String? @map("keykrtrn") @db.Char(15) // Refers to Transaktion, aligning with Transaction
  contractId String? @map("keyhyobj") @db.Char(15) // Previously known as Contract from converted models
  autogiroType Int @map("logtype") @db.TinyInt
  signature String @map("signature") @db.VarChar(80) // Based on Signatur
  autogiroDate DateTime @map("logdate") @db.DateTime
  autogiroTime DateTime @map("logtime") @db.DateTime
  transactionDate DateTime @map("transdate") @db.DateTime
  description String @map("caption") @db.VarChar(120) // Based on Benämning aligning with Description
  mandateId String? @map("mandateid") @db.VarChar(20) // Direct translation
  information String? @map("info") @db.VarChar(Max) // Translated from Beskrivning to Information
  timestamp String @map("timestamp") @db.Char(10) // TODO: Clarify if timestamp has a specific use or required renaming in context

  contact Contact? @relation(fields: [keycmctc], references: [keycmctc], onUpdate: NoAction, map: "fkkraglkeycmctc")
  contract Contract? @relation(fields: [keyhyobj], references: [keyhyobj], onUpdate: NoAction, map: "fkkraglkeyhyobj")
  krfkh krfkh? @relation(fields: [keykrfkh], references: [keykrfkh], onDelete: Cascade, onUpdate: NoAction, map: "fkkraglkeykrfkh")
  krtrn krtrn? @relation(fields: [keykrtrn], references: [keykrtrn], onDelete: Cascade, onUpdate: NoAction, map: "fkkraglkeykrtrn")

  @@index([contactId], map: "fkkraglcmctc")
  @@index([contractId], map: "fkkraglhyobj")
  @@index([invoiceId], map: "fkkraglkrfkh")
  @@index([transactionId], map: "fkkraglkrtrn")
  @@index([userId], map: "fkkraglsyusr")
}