model MaintenanceLog {
  @@map("pusta")

  maintenanceLogId String @id(map: "pkpusta") @map("keypusta") @db.Char(15)
  plannedMaintenanceId String? @map("keypuund") @db.Char(15)
  userId String? @map("keysyusr") @db.Char(15)
  objectId String? @map("keycmobj") @db.Char(15)
  actionId String? @map("keypuatg") @db.Char(15)
  subProjectId String? @map("keyprppr") @db.Char(15)
  latestExecutionYear Int @default(0, map: "DF__pusta__senastutf__00925B61") @map("senastutf") @db.SmallInt
  latestExecutionMonth Int? @map("senastutm") @db.TinyInt
  originalLastExecutionYear Int @default(0, map: "DF__pusta__orgsenutf__01867F9A") @map("orgsenutf") @db.SmallInt
  originalLastExecutionMonth Int? @map("orgsenutm") @db.TinyInt
  action String @map("atgard") @db.VarChar(30)
  description1 String? @map("text1") @db.VarChar(30)
  description2 String? @map("text2") @db.VarChar(30)
  plainText String? @map("klartext") @db.VarChar(30)
  costAmount Int? @map("kostnad")
  quantity Decimal? @map("mangd") @db.Decimal(9, 1)
  isInvestment Int @default(0, map: "DF__pusta__invest__027AA3D3") @map("invest") @db.TinyInt
  costFactor Decimal @default(1, map: "DF__pusta__costfacto__036EC80C") @map("costfactor") @db.Decimal(5, 4)
  date DateTime? @map("fdate") @db.DateTime
  timestamp String @map("timestamp") @db.Char(10)

  propertyObject PropertyObject? @relation(fields: [objectId], references: [propertyObjectId], onDelete: NoAction, onUpdate: NoAction, map: "fkpustakeycmobj")
  subProject SubProject? @relation(fields: [subProjectId], references: [subProjectId], onUpdate: NoAction, onDelete: NoAction, map: "fkpustakeyprppr")
  maintenanceAction MaintenanceAction? @relation(fields: [actionId], references: [maintenanceActionId], onUpdate: NoAction, map: "fkpustakeypuatg ")
  plannedMaintenance PlannedMaintenance? @relation(fields: [plannedMaintenanceId], references: [plannedMaintenanceId], onUpdate: NoAction, onDelete: NoAction, map: "fkpustakeypuund ")

  // TODO: Make sure userId relation is added later as the user model is not defined
  // @@index([userId], map: "fkpustasyusr")
  @@index([objectId], map: "fkpustacmobj")
  @@index([subProjectId], map: "fkpustaprppr")
  @@index([actionId], map: "fkpustapuatg")
  @@index([plannedMaintenanceId], map: "fkpustapuund")
}