model Favorite {
  @@map("cmunf")

  favoriteId String @id(map: "pkcmunf") @map("keycmunf") @db.Char(15)
  favoriteReferenceId String? @map("keycmunf2") @db.Char(15)
  groupId String @map("keycmung") @db.Char(15)
  typeId String? @map("keycmunt") @db.Char(15)
  textClusterId String? @map("keysytxc") @db.Char(15)
  selectionId String? @map("keycmqry") @db.Char(15)
  code String? @map("keycode") @db.Char(15)
  codeReference String? @map("keycode2") @db.Char(15)
  name String? @map("caption") @db.VarChar(200)
  description String? @map("descript") @db.VarChar(2000)
  // TODO: Determine the exact usage of ftype
  contentType String? @map("ctype") @db.VarChar(400)
  objectType Int @default(0, map: "DF__cmunf__omtid__3C6FBB86") @map("omtid")
  sortOrder Int @default(0, map: "DF__cmunf__sortorder__3D63DFBF") @map("sortorder")
  isPublished Int @default(0, map: "DF__cmunf__ispublish__3E5803F8") @map("ispublish") @db.TinyInt
  isUserDefined Int @default(0, map: "DF__cmunf__userdef__3F4C2831") @map("userdef") @db.TinyInt
  shareStatus Int @default(0, map: "DF__cmunf__sharestat__40404C6A") @map("sharestat") @db.TinyInt
  defaultWorkspace Int @default(0, map: "DF__cmunf__defaultws__413470A3") @map("defaultws") @db.TinyInt
  defaultWebWorkspace Int @default(0, map: "DF__cmunf__defaultww__422894DC") @map("defaultwws") @db.TinyInt
  defaultMobileWorkspace Int @default(0, map: "DF__cmunf__defaultmw__431CB915") @map("defaultmws") @db.TinyInt
  defaultApp Int @default(0, map: "DF__cmunf__defaultap__4410DD4E") @map("defaultapp") @db.TinyInt
  useCount Int @default(0, map: "DF__cmunf__timesused__45050187") @map("timesused")
  lastUsedDate DateTime? @map("lastused") @db.DateTime
  timestamp String @map("timestamp") @db.Char(10)

  cmunc_cmunc_keycmunfTocmunf cmunc[] @relation("cmunc_keycmunfTocmunf")
  cmunc_cmunc_keycmunf2Tocmunf cmunc[] @relation("cmunc_keycmunf2Tocmunf")
  // TODO: Change cmqry to new model name when available
  cmqry cmqry? @relation(fields: [selectionId], references: [keycmqry], onUpdate: NoAction, map: "fkcmunfkeycmqry ")
  // TODO: Change cmung to new model name when available
  cmung cmung @relation(fields: [groupId], references: [keycmung], onDelete: Cascade, onUpdate: NoAction, map: "fkcmunfkeycmung ")

  @@index([selectionId], map: "fkcmunfcmqry")
  @@index([groupId], map: "fkcmunfcmung")
  @@index([textClusterId], map: "fkcmunfsytxc")
}