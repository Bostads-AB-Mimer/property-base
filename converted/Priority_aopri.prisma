model Priority {
  @@map("aopri")
  
  id String @id(map: "pkaopri") @map("keyaopri") @db.Char(15)
  resourcePlanningType String? @map("keybaret") @db.Char(15) // TODO: change baret to new model name when available
  code String @unique(map: "akaopri") @map("code") @db.VarChar(30)
  maxDueDays Int? @map("tidmax")
  timeUnit String? @map("tidregel") @db.Char(1)
  sendAlertSms Int @default(0, map: "DF__aopri__alertsms__222D7900") @map("alertsms") @db.TinyInt
  mandatoryResource Int @default(0, map: "DF__aopri__demreso__23219D39") @map("demreso") @db.TinyInt
  colorScheme Int @default(0, map: "DF__aopri__plancolor__2415C172") @map("plancolor") @db.TinyInt
  planPriority Int @default(0, map: "DF__aopri__planprio__2509E5AB") @map("planprio") @db.TinyInt
  printOrder Int @default(0, map: "DF__aopri__printsort__25FE09E4") @map("printsort") @db.SmallInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  // TODO: change these virtual fields when new model names are available
  aokat aokat[] 
  baret baret? @relation(fields: [resourcePlanningType], references: [keybaret], onUpdate: NoAction, map: "fkaoprikeybaret")
  aoupp aoupp[]
  prprj prprj[]

  @@index([resourcePlanningType], map: "fkaopribaret")
}