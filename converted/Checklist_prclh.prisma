model Checklist {
  @@map("prclh")

  checklistId String @id(map: "pkprclh") @map("keyprclh") @db.Char(15)
  function Int @default(0, map: "DF__prclh__func__76E6A812") @map("func") @db.TinyInt
  name String @unique(map: "akprclh") @map("caption") @db.VarChar(100)
  description String @map("descript") @db.VarChar(1000)
  closeAllRows Int @default(1, map: "DF__prclh__clallrows__77DACC4B") @map("clallrows") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  queueTypes QueueType[] @relation("fkbkktykeyprclh") // Assuming this is the correct relation for queueTypes
  subProjects SubProject[] @relation("fkprpprkeyprclh", fields: [checklistId], references: [checklistId]) 
  projects Project[] @relation("fkprprjkeyprclh", fields: [checklistId], references: [checklistId])
}