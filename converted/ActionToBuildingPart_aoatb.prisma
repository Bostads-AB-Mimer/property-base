  // relatedAction Action @relation(fields: [actionId], references: [id], map: "fkaoatbkeypuatg") // updated the related model name

  @@unique([buildingPartId, actionId], map: "akaoatb1")
  @@index([buildingPartId], map: "fkaoatbaobdl")
  @@index([actionId], map: "fkaoatbpuatg")
}

