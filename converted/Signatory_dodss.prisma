model Signatory {
  @@map("dodss")

  signatoryId String @id(map: "pkdodss") @map("keydodss") @db.Char(15)
  digitalSignatureId String @map("keydodse") @db.Char(15)
  contactId String? @map("keycmctc") @db.Char(15)
  signOrder Int @default(0, map: "DF__dodss__signorder__0E3D456A") @map("signorder") 
  name String @map("name") @db.VarChar(100)
  company String? @map("company") @db.VarChar(50)
  email String? @map("email") @db.VarChar(80)
  phone String? @map("phone") @db.VarChar(30)
  personOrOrgNo String? @map("civicno") @db.VarChar(15)
  signDate DateTime? @map("signdate") @db.DateTime
  rejectDate DateTime? @map("rejectdate") @db.DateTime
  deliveryMethod Int @map("deliverymd") @db.TinyInt
  authorizedSign Int @map("authsign") @db.TinyInt
  authorizedView Int @map("authview") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  cmctc cmctc? @relation(fields: [contactId], references: [keycmctc], onUpdate: NoAction, map: "fkdodsskeycmctc")
  dodse dodse @relation(fields: [digitalSignatureId], references: [keydodse], onUpdate: NoAction, map: "fkdodsskeydodse")
  
  @@index([digitalSignatureId], map: "ix_akdodsskeydodse")
  @@index([contactId], map: "ix_dodsskeycmctc")
}