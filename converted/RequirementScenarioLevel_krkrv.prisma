model RequirementScenarioLevel {
  @@map("krkrv")
  requirementScenarioLevelId String @id(map: "pkkrkrv") @map("keykrkrv") @db.Char(15)
  requirementScenarioId String @map("keykrsce") @db.Char(15)
  transactionTypeId String? @map("keyrevrt") @db.Char(15)
  billingTypeId String? @map("keyfadbt") @db.Char(15)
  order Int @map("num") @db.SmallInt
  requirementType Int @map("type") @db.SmallInt
  name String? @map("name") @db.VarChar(30)
  description String? @map("text") @db.VarChar(30) // TODO: Confirm if 'text' is best represented as description
  collectionAgency Int @map("collfirm") @db.TinyInt
  feeNotice Int @map("feenotice") @db.TinyInt
  feeAmount Float @map("fee") @db.Money
  daysAfterDue Int @default(0, map: "DF__krkrv__days__1FBCD727") @map("days")
  gracePeriodDays Int @default(0, map: "DF__krkrv__resdays__20B0FB60") @map("resdays") @db.TinyInt
  minimumAmount Float @map("minamount") @db.Money
  minimumRentPercentage Float @default(0, map: "DF__krkrv__minrentpc__21A51F99") @map("minrentpct") @db.Money
  maximumAmount Float? @map("maxamount") @db.Money
  isBundled Int @map("bundle") @db.TinyInt
  bundleContentCount Int @default(0, map: "DF__krkrv__bundlecon__229943D2") @map("bundlecont") @db.TinyInt
  letterToPledgor Int @default(0, map: "DF__krkrv__pledgelet__238D680B") @map("pledgelet") @db.TinyInt
  letterToGuarantor Int @default(0, map: "DF__krkrv__guaranlet__24818C44") @map("guaranlet") @db.TinyInt
  letterToAllTenants Int @default(0, map: "DF__krkrv__allconlet__2575B07D") @map("allconlet") @db.TinyInt
  invoiceInformationBySms Int @default(0, map: "DF__krkrv__smsinv__2669D4B6") @map("smsinv") @db.TinyInt
  emailInvoiceInformation Int @default(0, map: "DF__krkrv__emailinv__275DF8EF") @map("emailinv") @db.TinyInt
  isActive Int @map("krvused") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: determine how this timestamp is used
  
  printSettings PrintSetting[] @relation("fkfaprnkeykrkrv") // Updated relation to use the new model name PrintSetting
  
  @@index([billingTypeId], map: "fkkrkrvfadbt")
  @@index([requirementScenarioId], map: "fkkrkrvkrsce")
  @@index([transactionTypeId], map: "fkkrkrvrevrt")
}