model KeyMapping {
  @@map("nykop")
  id String @id(map: "pknykop") @map("keynykop") @db.Char(15)
  keyType String @map("keynynty") @db.Char(15)
  cylinderType String @map("keynycty") @db.Char(15)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  cylinderTypeRelation nycty @relation(fields: [cylinderType], references: [keynycty], onUpdate: NoAction, map: "fknykopkeynycty ")
  keyTypeRelation nynty @relation(fields: [keyType], references: [keynynty], onUpdate: NoAction, map: "fknykopkeynynty ")

  @@index([cylinderType], map: "fknykopnycty")
  @@index([keyType], map: "fknykopnynty")
}