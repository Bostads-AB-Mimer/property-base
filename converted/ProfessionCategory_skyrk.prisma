model ProfessionCategory {
  @@map("skyrk")
  
  professionCategoryId String @id(map: "pkskyrk") @map("keyskyrk") @db.Char(15)
  name String @map("caption") @db.Char(30)
  hourlyCost Float @default(0, map: "DF__skyrk__amount__65145E1E") @map("amount") @db.Money
  workCategory Int @default(0, map: "DF__skyrk__ubtimecat__66088257") @map("ubtimecat") @db.TinyInt
  systemStandard Int @default(0, map: "DF__skyrk__repabkod__66FCA690") @map("repabkod") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  resources Resource[] @relation("fkcmreskeyskyrk") // cmres refers to Resource model
  maintenanceActions MaintenanceAction[] @relation("fkskatgkeyskyrk") // skatg refers to MaintenanceAction model
  maintenancePackageContents MaintenancePackageContent[] @relation("fkskpinkeyskyrk") // skpin refers to MaintenancePackageContent model
  periodicWorks PeriodicWork[] @relation("fkskundkeyskyrk") // skund refers to PeriodicWork model
}