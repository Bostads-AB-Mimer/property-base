model WaitingListTypeMessageTemplate {
  @@map("bkmtc")

  id String @id(map: "pkbkmtc") @map("keybkmtc") @db.Char(15)
  // Key to KÃ¶typ (Queue Type), maintain the original mapping
  waitingListTypeId String @map("keybkkty") @db.Char(15)
  // Key to Meddelandemall (Message Template), maintain the original mapping
  messageTemplateId String @map("keycrumt") @db.Char(15)
  isDefault Int @default(0, map: "DF__bkmtc__isdefault__7FC35263") @map("isdefault") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  // TODO: change bkkty to WaitingListType when available
  waitingListType bkkty @relation(fields: [waitingListTypeId], references: [keybkkty], onDelete: Cascade, onUpdate: NoAction, map: "fkbkmtckeybkkty")
  // TODO: change crumt to MessageTemplate when available
  messageTemplate crumt @relation(fields: [messageTemplateId], references: [keycrumt], onUpdate: NoAction, map: "fkbkmtckeycrumt")

  @@index([waitingListTypeId], map: "fkbkmtcbkkty")
  @@index([messageTemplateId], map: "fkbkmtccrumt")
}