model ReportData {
  @@map("cmrbh")
  reportDataId String @id(map: "pkcmrbh") @map("keycmrbh") @db.Char(15)
  userId String? @map("keysyusr") @db.Char(15) // Maps to Användare (User)
  name String @map("caption") @db.VarChar(80) // Maps to Benämning
  dotNetSupportLevel Int @default(0, map: "DF__cmrbh__dotnet__5447774B") @map("dotnet") @db.TinyInt
  isReadPermission Int @default(0, map: "DF__cmrbh__accr__553B9B84") @map("accr") @db.TinyInt // Maps to Läsa (Read)
  isWritePermission Int @default(0, map: "DF__cmrbh__accw__562FBFBD") @map("accw") @db.TinyInt // Maps to Ändra (Edit)
  isPublished Int @default(0, map: "DF__cmrbh__publish__5723E3F6") @map("publish") @db.TinyInt // Maps to Publicera (Publish)
  timestamp String @map("timestamp") @db.Char(10) // TODO: Review usage of timestamp for potential renaming
  
  batchProcessRows BatchProcessRow[] @relation("fkcmbtrkeycmrbh") // Updated reference based on previous conversion
  reportBatchRows ReportBatchRow[] @relation("fkcmrbrkeycmrbh") // Updated reference based on previous conversion

  @@index([userId], map: "fkcmrbhsyusr")
}