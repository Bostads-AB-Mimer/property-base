model WorkOrderCategory {
  @@map("aokat")

  workOrderCategoryId String @id(map: "pkaokat") @map("keyaokat") @db.Char(15)
  priorityId String? @map("keyaopri") @db.Char(15)
  workOrderPackageId String? @map("keyaopah") @db.Char(15)
  routeId String? @map("keyatway") @db.Char(15) // Assuming 'Färdväg' translates to 'routeId'
  resourcePlanningTypeId String? @map("keybaret") @db.Char(15)
  resourcePlanningTypeId2 String? @map("keybaret2") @db.Char(15)
  name String @unique(map: "akaokat") @map("caption") @db.VarChar(30)
  sendToManager Int @default(0, map: "DF__aokat__notiadmin__63BEC505") @map("notiadmin") @db.TinyInt
  requireModule Int @default(0, map: "DF__aokat__forcemdl__64B2E93E") @map("forcemdl") @db.TinyInt
  requirePriorityGroup Int @default(0, map: "DF__aokat__forceprg__65A70D77") @map("forceprg") @db.TinyInt
  requireDevelopmentProject Int @default(0, map: "DF__aokat__devprj__669B31B0") @map("devprj") @db.TinyInt
  printOrder Int @default(0, map: "DF__aokat__printsort__678F55E9") @map("printsort") @db.SmallInt
  publish Int @default(0, map: "DF__aokat__publish__68837A22") @map("publish") @db.TinyInt
  isActive Int @default(1, map: "DF__aokat__isactive__69779E5B") @map("isactive") @db.TinyInt
  emergencyNumber String? @map("smsalarm") @db.VarChar(60)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  // TODO: Change these relationships when new model names are available
  aopah aopah? @relation(fields: [workOrderPackageId], references: [keyaopah], onUpdate: NoAction, map: "fkaokatkeyaopah")
  priority aopri? @relation(fields: [priorityId], references: [keyaopri], onUpdate: NoAction, onDelete: NoAction, map: "fkaokatkeyaopri")
  resourcePlanningType baret_aokat_keybaretTobaret baret? @relation("aokat_keybaretTobaret", fields: [resourcePlanningTypeId], references: [keybaret], onUpdate: NoAction, map: "fkaokatkeybaret")
  resourcePlanningType2 baret_aokat_keybaret2Tobaret baret? @relation("aokat_keybaret2Tobaret", fields: [resourcePlanningTypeId2], references: [keybaret], onDelete: NoAction, onUpdate: NoAction, map: "fkaokatkeybaret2")
  aokax aokax[] // TODO: determine what 'aokax' represents and consider renaming
  workOrderCategoryLinks aotyc[] // Assuming 'aotyc' relates to 'WorkOrderCategoryLinks' based on previous conversion
  workOrders aoupp[] // Assuming 'aoupp' represents work orders based on previous conversion
  tvpdg tvpdg[] // TODO: determine what 'tvpdg' represents and consider renaming
  
  @@index([workOrderPackageId], map: "fkaokataopah")
  @@index([priorityId], map: "fkaokataopri")
  @@index([resourcePlanningTypeId], map: "fkaokatbaret")
  @@index([resourcePlanningTypeId2], map: "fkaokatbaret2")
}