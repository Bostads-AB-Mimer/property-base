model ActionRentalRow {
  @@map("vlutf")

  actionRentalRowId String @id(map: "pkvlutf") @map("keyvlutf") @db.Char(15)
  propertyObjectId String @map("keycmobj") @db.Char(15) // Based on previous conversion "Objekt"
  contractId String? @map("keyhyobj") @db.Char(15) // Based on the domain reference "Kontrakt"
  rentalRowId String? @map("keyhyrad") @db.Char(15) // Based on the previous conversion "Hyresrad"
  repaymentPeriod Int @map("period") @db.TinyInt // Renamed for clarity based on domain knowledge
  interestRate Decimal @map("interest") @db.Decimal(4, 2) // Renamed for clarity based on domain knowledge
  totalAmount Float @default(0, map: "DF__vlutf__amount__5315A974") @map("amount") @db.Money // Renamed for clarity
  fromDate DateTime @map("fdate") @db.DateTime // Based on the domain reference "Fr.o.m."
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  propertyObject PropertyObject @relation(fields: [propertyObjectId], references: [propertyObjectId], onDelete: Cascade, onUpdate: NoAction, map: "fkvlutfkeycmobj")
  contract Contract? @relation(fields: [contractId], references: [contractId], onDelete: NoAction, onUpdate: NoAction, map: "fkvlutfkeyhyobj")
  rentalRow RentalRow? @relation(fields: [rentalRowId], references: [rentalRowId], onDelete: Cascade, onUpdate: NoAction, map: "fkvlutfkeyhyrad")

  @@index([propertyObjectId], map: "fkvlutfcmobj")
  @@index([contractId], map: "fkvlutfhyobj")
  @@index([rentalRowId], map: "fkvlutfhyrad")
}