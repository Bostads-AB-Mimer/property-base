model PropertyType {
  @@map("balot")

  propertyTypeId String @id(map: "pkbalot") @map("keybalot") @db.Char(15)
  componentTypeActionId String? @map("keypuatt") @db.Char(15)
  statisticsGroupScbId String? @map("keyballs") @db.Char(15)
  statisticsGroup2Id String? @map("keyballs2") @db.Char(15)
  statisticsGroup3Id String? @map("keyballs3") @db.Char(15)
  statisticsGroup4Id String? @map("keyballs4") @db.Char(15)
  assetManagementPropertyTypeId String? @map("keyamvat") @db.Char(15)
  checklistId String? @map("keylbpro") @db.Char(15)
  code String @unique(map: "akbalot") @map("code") @db.VarChar(15)
  name String? @map("caption") @db.VarChar(60)
  usage Int @default(0, map: "DF__balot__usage__4A5B65EB") @map("usage") @db.TinyInt
  isSystemStandard Int @default(0, map: "DF__balot__issystem__4B4F8A24") @map("issystem") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: consider renaming timestamp field when knowing more about its usage

  balok balok[] // TODO: rename balok to new model name when available
  statisticsGroupScb balot_keyballsTostatisticsGroupScb? @relation("balot_keyballsToballs", fields: [statisticsGroupScbId], references: [keyballs], onUpdate: NoAction, map: "fkbalotkeyballs")
  statisticsGroup2 balot_keyballs2StatisticsGroup2 balls? @relation("balot_keyballs2Toballs", fields: [statisticsGroup2Id], references: [keyballs], onDelete: NoAction, onUpdate: NoAction, map: "fkbalotkeyballs2")
  statisticsGroup3 balot_keyballs3StatisticsGroup3 balls? @relation("balot_keyballs3Toballs", fields: [statisticsGroup3Id], references: [keyballs], onDelete: NoAction, onUpdate: NoAction, map: "fkbalotkeyballs3")
  statisticsGroup4 balot_keyballs4StatisticsGroup4 balls? @relation("balot_keyballs4Toballs", fields: [statisticsGroup4Id], references: [keyballs], onDelete: NoAction, onUpdate: NoAction, map: "fkbalotkeyballs4")
  checklist lbpro? @relation(fields: [checklistId], references: [keylbpro], onUpdate: NoAction, map: "fkbalotkeylbpro")
  componentTypeAction puatt? @relation(fields: [componentTypeActionId], references: [keypuatt], onUpdate: NoAction, map: "fkbalotkeypuatt")

  @@index([statisticsGroupScbId], map: "fkbalotballs")
  @@index([statisticsGroup2Id], map: "fkbalotballs2")
  @@index([statisticsGroup3Id], map: "fkbalotballs3")
  @@index([statisticsGroup4Id], map: "fkbalotballs4")
  @@index([checklistId], map: "fkbalotlbpro")
  @@index([componentTypeActionId], map: "fkbalotpuatt")
}