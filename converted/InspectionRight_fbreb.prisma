model InspectionRight {
  @@map("fbreb")

  inspectionRightId String @id(map: "pkfbreb") @map("keyfbreb") @db.Char(15)
  inspectorId String @map("keycmctc") @db.Char(15)
  inspectionTypeId String @map("keycmtyp") @db.Char(15)
  permissionTypeId String @map("keyfbbeh") @db.Char(15)
  timestamp String @map("timestamp") @db.Char(10) // TODO: Confirm if 'timestamp' needs a different naming convention or purpose

  // Relations
  inspector cmctc @relation(fields: [inspectorId], references: [keycmctc], onUpdate: NoAction, map: "fkfbrebkeycmctc") // TODO: Change 'cmctc' to the new model name when available
  inspectionType cmtyp @relation(fields: [inspectionTypeId], references: [keycmtyp], onUpdate: NoAction, map: "fkfbrebkeycmtyp") // TODO: Change 'cmtyp' to the new model name when available
  permissionType fbbeh @relation(fields: [permissionTypeId], references: [keyfbbeh], onUpdate: NoAction, map: "fkfbrebkeyfbbeh") // TODO: Change 'fbbeh' to the new model name when available

  @@index([inspectorId], map: "fkfbrebcmctc")
  @@index([inspectionTypeId], map: "fkfbrebcmtyp")
  @@index([permissionTypeId], map: "fkfbrebfbbeh")
}