model WorkOrderType {
  @@map("aotyp")

  workOrderTypeId String @id(map: "pkaotyp") @map("keyaotyp") @db.Char(15)
  workflowId String? @map("keyaowfd") @db.Char(15)
  resourcePlanningTypeId String? @map("keybaret") @db.Char(15)
  name String @unique(map: "akaotyp") @map("caption") @db.VarChar(60)
  includeInSummary Int @default(0, map: "DF__aotyp__incsum__31F4ACCB") @map("incsum") @db.TinyInt
  autoNotify Int @default(1, map: "DF__aotyp__autonoti__32E8D104") @map("autonoti") @db.TinyInt
  printOrder Int @default(0, map: "DF__aotyp__printsort__33DCF53D") @map("printsort") @db.SmallInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: re-evaluate the use of "timestamp" later
  
  // TODO: update associations when new model names are available
  aoacc aoacc[] 
  aotyc aotyc[]

  // Relations with potential new names
  baret baret? @relation(fields: [resourcePlanningTypeId], references: [keybaret], onUpdate: NoAction, map: "fkaotypkeybaret")
  aoupp aoupp[]

  @@index([resourcePlanningTypeId], map: "fkaotypbaret")
}