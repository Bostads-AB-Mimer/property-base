model InspectionCategory {
  @@map("fbbka")

  inspectionCategoryId String @id(map: "pkfbbka") @map("keyfbbka") @db.Char(15)
  standardProtocolId String? @map("keyfbbla") @db.Char(15)
  inspectionTypeId String @map("keycmtyp") @db.Char(15)
  unitId String @map("keycmuni") @db.Char(15)
  unitReInspectionIntervalId String? @map("keycmuni2") @db.Char(15)
  inspectionCategoryCode String @map("code") @db.VarChar(30)
  name String? @map("caption") @db.VarChar(80)
  interval Int @default(0, map: "DF__fbbka__intervall__4E760F92") @map("intervall") @db.TinyInt
  decidedInspectionDate Int @default(0, map: "DF__fbbka__lockdate__4F6A33CB") @map("lockdate") @db.TinyInt
  recurring Int @default(1, map: "DF__fbbka__recurr__505E5804") @map("recurr") @db.TinyInt
  reInspectionInterval Int @default(0, map: "DF__fbbka__reinstrge__51527C3D") @map("reinstrge") @db.TinyInt
  fireDrill Int @default(0, map: "DF__fbbka__firedrill__5246A076") @map("firedrill") @db.TinyInt
  active Int @default(1, map: "DF__fbbka__bkaactive__533AC4AF") @map("bkaactive") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  fbbes fbbes[] // TODO: change fbbes to new model name when available
  cmtyp InspectionType @relation(fields: [inspectionTypeId], references: [keycmtyp], onUpdate: NoAction, map: "fkfbbkakeycmtyp")
  cmuni_fbbka_keycmuniTocmuni Unit @relation("fbbka_keycmuniTocmuni", fields: [unitId], references: [unitId], onUpdate: NoAction, map: "fkfbbkakeycmuni ")
  cmuni_fbbka_keycmuni2Tocmuni Unit? @relation("fbbka_keycmuni2Tocmuni", fields: [unitReInspectionIntervalId], references: [unitId], onDelete: NoAction, onUpdate: NoAction, map: "fkfbbkakeycmuni2 ")
  fbbla fbbla? @relation(fields: [standardProtocolId], references: [standardProtocolId], onUpdate: NoAction, map: "fkfbbkakeyfbbla")
  fbbkb InspectionCategoryPermission[] // Updated model relationship reference
  fbgod fbgod[] // TODO: change fbgod to new model name when available

  @@index([inspectionTypeId], map: "fkfbbkacmtyp")
  @@index([unitId], map: "fkfbbkacmuni")
  @@index([unitReInspectionIntervalId], map: "fkfbbkacmuni2")
  @@index([standardProtocolId], map: "fkfbbkafbbla")
}