model PlannedMaintenance {
  id                   String    @id(map: "pkpuund") @map("keypuund") @db.Char(15)
  propertyObjectId     String    @map("keycmobj") @db.Char(15)
  maintenanceActionId  String    @map("keypuatg") @db.Char(15)
  measurementTypeId    String?   @map("keycmvat") @db.Char(15)
  actionGroupId        String?   @map("keypuagr") @db.Char(15)
  subProjectId         String?   @map("keyprppr") @db.Char(15)
  rentalAgreementId    String?   @map("keyhyobj") @db.Char(15)
  projectId            String?   @map("keyprprj") @db.Char(15)
  nextExecutionYear    Int?      @map("nastautf") @db.SmallInt
  intervalYears        Int?      @map("intervall") @db.TinyInt
  lastExecutionYear    Int?      @map("senastutf") @db.SmallInt
  lastExecutionMonth   Int?      @map("senastutm") @db.TinyInt
  stopYear             Int?      @map("stopp") @db.SmallInt
  simulatedNextYear    Int?      @map("simnasta") @db.SmallInt
  simulatedInterval    Int?      @map("siminterv") @db.TinyInt
  preliminaryNextYear  Int?      @map("prelnext") @db.SmallInt
  quantity             Decimal?  @map("mangd") @db.Decimal(9, 1)
  deferred             Int?      @map("framflytt") @db.TinyInt
  priority             Int?      @map("prioritet") @db.TinyInt
  difficultyLevel      Decimal?  @map("svarighet") @db.Decimal(6, 2)
  note                 String?   @map("text") @db.VarChar(120)
  type                 String?   @map("typ") @db.Char(2)
  status               Int       @default(0, map: "DF__puund__status__013179DF") @map("status") @db.TinyInt
  isInvestment         Int       @default(0, map: "DF__puund__invest__02259E18") @map("invest") @db.TinyInt
  extraNote            String?   @map("extranote") @db.VarChar(60)
  costFactor           Decimal   @default(1, map: "DF__puund__costfacto__0319C251") @map("costfactor") @db.Decimal(5, 4)
  apartmentMaintenance Int       @default(0, map: "DF__puund__hglu__040DE68A") @map("hglu") @db.TinyInt
  ordered              String?   @map("bestalld") @db.Char(1)
  orderDate            DateTime? @map("bestdat") @db.DateTime
  costsChargedToTenant Int?      @map("hgluhgbet") @db.TinyInt
  rescheduled          Int       @default(0, map: "DF__puund__hgludeb__05020AC3") @map("hgludeb") @db.SmallInt
  deduction            Int?      @map("hgluavdrag") @db.SmallInt
  vluExtensionDate     DateTime? @map("hglutdate") @db.DateTime
  executedUntil        Int?      @map("hgluxed") @db.TinyInt
  approvedFromDate     DateTime? @map("apprfdate") @db.DateTime
  approvedToDate       DateTime? @map("apprtdate") @db.DateTime
  approvalNote         String?   @map("apprnote") @db.VarChar(250)
  // approvedBy           Signature?     @map("apprsign") @db.VarChar(10)
  approvalDate         DateTime? @map("apprdate") @db.DateTime
  // rejectedBy           Signature?     @map("rejsign") @db.VarChar(10)
  rejectionDate        DateTime? @map("rejdate") @db.DateTime
  startMonth           Int?      @map("startmonth") @db.TinyInt
  executionMonth       Int?      @map("finmonth") @db.TinyInt
  // incomeChange         Turnover?      @map("incchange") @db.Money
  timestamp            String    @map("timestamp") @db.Char(10)

  // todo: Add relations based on the existing models
  // propertyObject       PropertyObject @relation(fields: [propertyObjectId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fkpuundkeycmobj")
  // maintenanceLogs      MaintenanceLog[] @relation("pusta_keypuundTopuund")
  // measurementType      MeasurementType?        @relation(fields: [measurementTypeId], references: [keycmvat], onDelete: NoAction, onUpdate: NoAction, map: "fkpuundkeycmvat ")
  // rentalAgreement      Contract?               @relation(fields: [rentalAgreementId], references: [contractId], onUpdate: NoAction, map: "fkpuundkeyhyobj")
  // subProject           SubProject?             @relation(fields: [subProjectId], references: [subProjectId], onUpdate: NoAction, onDelete: NoAction, map: "fkpuundkeyprppr")
  // project              Project?                @relation(fields: [projectId], references: [projectId], onUpdate: NoAction, onDelete: NoAction, map: "fkpuundkeyprprj")
  // actionGroup          MaintenanceActionGroup? @relation(fields: [actionGroupId], references: [maintenanceActionGroupId], onUpdate: NoAction, map: "fkpuundkeypuagr")
  maintenanceAction MaintenanceAction @relation(fields: [maintenanceActionId], references: [id], onUpdate: NoAction, map: "fkpuundkeypuatg ")

  @@index([propertyObjectId], map: "fkpuundcmobj")
  @@index([measurementTypeId], map: "fkpuundcmvat")
  @@index([rentalAgreementId], map: "fkpuundhyobj")
  @@index([subProjectId], map: "fkpuundprppr")
  @@index([projectId], map: "fkpuundprprj")
  @@index([actionGroupId], map: "fkpuundpuagr")
  @@index([maintenanceActionId], map: "fkpuundpuatg")
  @@map("puund")
}
