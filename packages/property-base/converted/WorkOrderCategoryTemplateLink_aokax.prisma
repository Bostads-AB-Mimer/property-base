model WorkOrderCategoryTemplateLink {
  @@map("aokax")

  // Fields
  workOrderCategoryTemplateLinkId String @id(map: "pkaokax") @map("keyaokax") @db.Char(15)
  workOrderCategoryId String @map("keyaokat") @db.Char(15)
  namingTemplateId String @map("keyaoctx") @db.Char(15)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

