model InspectionProtocol {
  @@map("lbpro")
  
  inspectionProtocolId String @id(map: "pklbpro") @map("keylbpro") @db.Char(15)
  name String @unique(map: "aklbpro1") @map("caption") @db.VarChar(30)
  isChecklist Int @default(0, map: "DF__lbpro__ischklst__44DF07B2") @map("ischklst") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: consider changing timestamp later when knowing more about its usage

  residences Residence[] @relation("fkbalghkeylbpro") // Changed from balgh to new model name Residence
  residenceTypes ResidenceType[] @relation("fkbalgtkeylbpro") // Changed from balgt to new model name ResidenceType
  locations Location[] @relation("fkbalokkeylbpro") // Changed from balok to new model name Location
  propertyTypes PropertyType[] @relation("fkbalotkeylbpro") // Changed from balot to new model name PropertyType
  inspectionCategories InspectionCategory[] @relation("fklbbkakeylbpro") // Changed from lbbka to new model name InspectionCategory
  inspectionProtocolPlacementConnections InspectionProtocolPlacementConnection[] @relation("fklbplakeylbpro") // Changed from lbpla to new model name InspectionProtocolPlacementConnection
}