model CartRow {
  @@map("tvscr")

  cartRowId String @id(map: "pktvscr") @map("keytvscr") @db.Char(15)
  cartId String @map("keytvsch") @db.Char(15)
  priceCategoryConnectionId String @map("keytvpcc") @db.Char(15)
  articleId String? @map("keytvart") @db.Char(15)
  propertyObjectId String? @map("keycmobj") @db.Char(15) // Previously converted to 'PropertyObject'
  alternativeComponentId String? @map("keycmobj2") @db.Char(15) // Previously converted to 'AlternativeComponent'
  deductionTypeId String? @map("keyprdct") @db.Char(15) // Possibly to be converted based on context
  status Int @default(0, map: "DF__tvscr__status__4D1069D7") @map("status") @db.TinyInt
  invoiceChoice Int @default(0, map: "DF__tvscr__invchoice__4E048E10") @map("invchoice") @db.TinyInt
  invoiceStatus Int @default(0, map: "DF__tvscr__invstatus__4EF8B249") @map("invstatus") @db.TinyInt
  extraProduct Int @default(0, map: "DF__tvscr__prdextra__4FECD682") @map("prdextra") @db.TinyInt
  quantityAmount Float @default(1, map: "DF__tvscr__quantity__50E0FABB") @map("quantity") @db.Money
  totalAmount Float @default(0, map: "DF__tvscr__amount__51D51EF4") @map("amount") @db.Money
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  propertyObject PropertyObject? @relation(fields: [propertyObjectId], references: [propertyObjectId], onUpdate: NoAction, map: "fktvscrkeycmobj ")
  article Item? @relation(fields: [articleId], references: [itemId], onDelete: NoAction, onUpdate: NoAction, map: "fktvscrkeytvart ") // Changed from tvart to Item
  priceCategoryConnection tvpcc @relation(fields: [priceCategoryConnectionId], references: [keytvpcc], onUpdate: NoAction, map: "fktvscrkeytvpcc ")
  cart tvsch @relation(fields: [cartId], references: [keytvsch], onDelete: Cascade, onUpdate: NoAction, map: "fktvscrkeytvsch ")

  @@index([propertyObjectId], map: "fktvscrcmobj")
  @@index([alternativeComponentId], map: "fktvscrcmobj2")
  @@index([articleId], map: "fktvscrtvart")
  @@index([priceCategoryConnectionId], map: "fktvscrtvpcc")
  @@index([cartId], map: "fktvscrtvsch")
}