  // placement Placement @relation(fields: [placementId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fkaoplokeyaopla") // TODO: verify naming

  // TODO: Verify if 'ObjectType' model reference 'id' correctly, if 'id' doesn't exist, comment out relations.
  // objectType ObjectType @relation(fields: [objectTypeId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fkaoplokeycmobt") // TODO: verify naming

  @@unique([placementId, objectTypeId], map: "akaoplo")
  @@index([objectTypeId], map: "fkaoplocmobt")
}