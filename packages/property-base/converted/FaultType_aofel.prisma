model FaultType {
  @@map("aofel")

  faultTypeId String @id(map: "pkaofel") @map("keyaofel") @db.Char(15)
  code String @unique(map: "akaofel1") @map("code") @db.VarChar(10)
  name String? @map("caption") @db.VarChar(70)
  publish Int @default(0, map: "DF__aofel__publish__30BC3164") @map("publish") @db.TinyInt
  isActive Int @default(1, map: "DF__aofel__isactive__31B0559D") @map("isactive") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  workOrderRow WorkOrderRow[] @relation("fkaoparkeyaofel")
  statistics Statistics[] @relation("fkaostakeyaoatg")
  workOrder WorkOrderRow[] @relation("fkaouprkeyaofel")
  
  // TODO: change aopar, aosta, and aoupr to new model names once they are confirmed
}