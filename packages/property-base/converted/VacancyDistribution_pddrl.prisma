model VacancyDistribution {
  @@map("pddrl")

  // Fields
  vacancyDistributionId String @id(map: "pkpddrl") @map("keypddrl") @db.Char(15)
  objectId String @map("keycmobj") @db.Char(15) // TODO: change cmobj to new model name when available
  articleId String? @map("keycmart") @db.Char(15) // TODO: cmart model name needs confirmation
  recipient Int @default(0, map: "DF__pddrl__recipient__347126C3") @map("recipient") @db.TinyInt
  calculationType Int @default(0, map: "DF__pddrl__calctype__35654AFC") @map("calctype") @db.TinyInt
  fromInterval Decimal? @map("rangefrom") @db.Decimal(7, 2)
  toInterval Decimal? @map("rangeto") @db.Decimal(7, 2)
  validFromDate DateTime? @map("validfrom") @db.DateTime
  validToDate DateTime? @map("validto") @db.DateTime
  percentage Decimal @map("prnt") @db.Decimal(7, 2)
  sortOrder Int @map("sortorder") // Order is renamed to sortOrder for clarification
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  // Relations
  article Article? @relation(fields: [articleId], references: [articleId], onDelete: Cascade, onUpdate: NoAction, map: "fkpddrlkeycmart")
  cmobj cmobj @relation(fields: [objectId], references: [keycmobj], onDelete: Cascade, onUpdate: NoAction, map: "fkpddrlkeycmobj")

  // Indexes
  @@index([articleId], map: "fkpddrlkeycmart")
  @@index([objectId], map: "fkpddrlkeycmobj")
}