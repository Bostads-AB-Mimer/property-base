model OrderingMethod {
  @@map("iksbg")
  
  id String @id(map: "pkiksbg") @map("keyiksbg") @db.Char(15) // Best채llningss채tt
  name String @unique(map: "akiksbg1") @map("caption") @db.VarChar(30) // Best채llningss채tt
  isEmail Int @default(0, map: "DF__iksbg__email__1535F2D4") @map("email") @db.TinyInt // Skickas via epost
  isPrinter Int @default(0, map: "DF__iksbg__printer__162A170D") @map("printer") @db.TinyInt // Skickas via skrivare
  timestamp String @map("timestamp") @db.Char(10) // TODO: Evaluate if timestamp should be more descriptive
  
  // Relations
  contract Contract[] @relation("fkhyobjkeyiksbg") // Changed from hyobj; previously referenced in Contract model
  order Order[] @relation("fkikbehkeyiksbg") // Changed from ikbeh; previously referenced in Order model
}