model MainGroup {
  @@map("skafa")

  id String @id(map: "pkskafa") @map("keyskafa") @db.Char(15)
  code String @map("code") @db.VarChar(10)
  name String @map("caption") @db.VarChar(120)
  systemStandard Int @default(0, map: "DF__skafa__repabkod__0806056A") @map("repabkod") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: evaluate if timestamp should remain or be renamed

  workOrders WorkOrderRow[] // Updated to match the correct model name from previous conversions
  // TODO: Find the correct relation naming for the maintenanceActions relationship
  maintenanceActions MaintenanceAction[] // Commented out the relation name since it seems inconsistent with generated relation naming
}