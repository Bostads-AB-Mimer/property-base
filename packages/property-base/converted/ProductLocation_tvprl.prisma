model ProductLocation {
  @@map("tvprl")
  
  productLocationId String @id(map: "pktvprl") @map("keytvprl") @db.Char(15)
  productId String @map("keytvprd") @db.Char(15)
  objectId String? @map("keycmobj") @db.Char(15)
  isAllowed Int @default(1, map: "DF__tvprl__prdallowe__34021078") @map("prdallowed") @db.TinyInt
  blockedFromDate DateTime? @map("blockfdate") @db.DateTime
  blockedToDate DateTime? @map("blocktdate") @db.DateTime
  timestamp String @map("timestamp") @db.Char(10) // TODO: consider changing timestamp later if its usage becomes clearer

  // Assuming new model name for cmobj would be given, update when available
  // TODO: rename cmobj to new model name when available
  cmobj cmobj? @relation(fields: [objectId], references: [keycmobj], onDelete: Cascade, onUpdate: NoAction, map: "fktvprlkeycmobj")
  // Updated relation for the converted model
  product Product @relation(fields: [productId], references: [keytvprd], onDelete: Cascade, onUpdate: NoAction, map: "fktvprlkeytvprd")

  @@unique([productId, objectId], map: "aktvprl1")
  @@index([objectId], map: "fktvprlcmobj")
}