model DataSource {
  @@map("bsend")
  
  dataSourceId String @id(map: "pkbsend") @map("keybsend") @db.Char(15)
  secondaryDataSourceId String? @map("keybsend2") @db.Char(15)
  blankettId String? @map("keybsdef") @db.Char(15) // TODO: Confirm exact related model name for "bsdef"
  selectionId String? @map("keycmqry") @db.Char(15) // Based on the model Selection in previous schemas
  name String @map("name") @db.VarChar(50)
  inputType Int @default(0, map: "DF__bsend__nodekind__04880780") @map("nodekind") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Consider changing to DateTime if applicable

  bsdef bsdef? @relation(fields: [blankettId], references: [keybsdef], onDelete: Cascade, onUpdate: NoAction, map: "fkbsendkeybsdef") // TODO: Update when new model name is confirmed
  dataSource DataSource? @relation("bsendTobsend", fields: [secondaryDataSourceId], references: [dataSourceId], onDelete: NoAction, onUpdate: NoAction, map: "fkbsendkeybsend2")
  otherDataSource DataSource[] @relation("bsendTobsend")
  cmqry Selection? @relation(fields: [selectionId], references: [selectionId], onDelete: NoAction, onUpdate: NoAction, map: "fkbsendkeycmqry")
  cmmap IntegrationSchema[] // Based on the model IntegrationSchema in previous schemas

  @@index([blankettId], map: "fkbsendkeybsdef")
  @@index([secondaryDataSourceId], map: "fkbsendkeybsend2")
  @@index([selectionId], map: "fkbsendkeycmqry")
}