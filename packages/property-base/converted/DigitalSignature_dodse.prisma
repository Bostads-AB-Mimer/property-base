model DigitalSignature {
  @@map("dodse")

  digitalSignatureId String @id(map: "pkdodse") @map("keydodse") @db.Char(15)
  documentId String @map("keydorev") @db.Char(15)
  userId String @map("keysyusr") @db.Char(15)
  createdDate DateTime @map("createdate") @db.DateTime
  sentDate DateTime? @map("senddate") @db.DateTime
  validToDate DateTime? @map("validto") @db.DateTime
  canceledDate DateTime? @map("canceldate") @db.DateTime
  externalDocumentId String? @map("exdocid") @db.VarChar(100)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage
  
  dorev dorev @relation(fields: [documentId], references: [keydorev], onDelete: Cascade, onUpdate: NoAction, map: "fkdodsekeydorev")
  dodss dodss[] // TODO: change dodss to new model name when available

  @@index([documentId], map: "ix_dodsekeydorev")
  @@index([userId], map: "ix_dodsekeysyusr")
  @@index([validToDate], map: "ix_dodsevalidto")
}