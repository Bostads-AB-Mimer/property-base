model DeliveryMethod {
  @@map("ikslr")

  id String @id(map: "PK__ikslr__546180BB") @map("keyikslr") @db.Char(15)
  name String @unique(map: "akikslr1") @map("caption") @db.Char(30)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  contracts Contract[] @relation("hyobj_keyikslr") // Ensure consistency with renamed Contract model
  orders Order[] @relation("ikbeh_keyikslr") // Ensure consistency with renamed Order model
}