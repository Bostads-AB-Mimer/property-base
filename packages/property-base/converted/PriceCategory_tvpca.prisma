model PriceCategory {
  @@map("tvpca")
  
  priceCategoryId String @id(map: "pktvpca") @map("keytvpca") @db.Char(15)
  unitId String? @map("keycmuni") @db.Char(15)
  name String @unique(map: "aktvpca") @map("caption") @db.VarChar(60)
  duration Int @default(0, map: "DF__tvpca__pcadura__2F8006F0") @map("pcadura") @db.SmallInt
  includesRenovationRent Int @default(0, map: "DF__tvpca__inrenoren__30742B29") @map("inrenorent") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Reconsider timestamp later if more information becomes available
  
  // TODO: change bakmp to new model name when available
  component Component[] // Previously bakmp, updated to new model Component in previous conversion
  
  unit Unit? @relation(fields: [unitId], references: [unitId], onUpdate: NoAction, map: "fktvpcakeycmuni") // Already converted to Unit in previous examples

  @@index([unitId], map: "fktvpcacmuni")
}