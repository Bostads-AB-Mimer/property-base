model Unit {
  @@map("cmuni")

  unitId String @id(map: "pkcmuni") @map("keycmuni") @db.Char(15)
  textClusterId String @map("keysytxc") @db.Char(15)
  systemStandardConversionTo String @map("tosi") @db.VarChar(Max)
  systemStandardConversionFrom String @map("fromsi") @db.VarChar(Max)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  // Relations (TODOs for pending new model names)
  cmart cmart[] @relation("cmart_keycmuniTocmuni")
  cmart cmart[] @relation("cmart_keycmuni2Tocmuni")
  gdprSetting GdprSetting[] // Updated from cmgdr
  fbbka fbbka[] @relation("fbbka_keycmuniTocmuni")
  fbbka fbbka[] @relation("fbbka_keycmuni2Tocmuni")
  hybev hybev[] @relation("hybev_keycmuniTocmuni")
  hybev hybev[] @relation("hybev_keycmuni2Tocmuni")
  hyobj hyobj[] @relation("hyobj_keycmuniTocmuni")
  hyobj hyobj[] @relation("hyobj_keycmuni2Tocmuni")
  hyobj hyobj[] @relation("hyobj_keycmuni3Tocmuni")
  rentalRow RentalRow[] // Previously hyrad
  hysec hysec[] @relation("hysec_keycmuniTocmuni")
  hysec hysec[] @relation("hysec_keycmuni2Tocmuni")
  hysec hysec[] @relation("hysec_keycmuni3Tocmuni")
  priceListRow PriceListRow[] // Updated from lbplr
  maintenanceAction MaintenanceAction[] // Previously puatg
  ledgerRow LedgerRow[] // Updated from revrr
  // TODO: Need clarification on other maintenanceAction relation
  // maintenanceAction MaintenanceAction[] @relation("skatg_keycmuniToskdmv")
  priceCategory PriceCategory[] // Updated from tvpca

  @@index([textClusterId], map: "fkcmunisytxc")
