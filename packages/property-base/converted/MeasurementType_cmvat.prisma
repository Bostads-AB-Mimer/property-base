  cmvap cmvap[] // TODO: Rename cmvap when available
  hyinf_hyinf_keycmvatTocmvat hyinf[] @relation("hyinf_keycmvatTocmvat") // TODO: Rename hyinf when available
  hyinf_hyinf_keycmvat2Tocmvat hyinf[] @relation("hyinf_keycmvat2Tocmvat") // TODO: Rename hyinf when available
  hyinf_hyinf_keycmvat3Tocmvat hyinf[] @relation("hyinf_keycmvat3Tocmvat") // TODO: Rename hyinf when available
  hyinf_hyinf_keycmvat4Tocmvat hyinf[] @relation("hyinf_keycmvat4Tocmvat") // TODO: Rename hyinf when available
  puatg_puatg_keycmvatTocmvat puatg[] @relation("puatg_keycmvatTocmvat") // TODO: Rename puatg when available
  puatg_puatg_keycmvat2Tocmvat puatg[] @relation("puatg_keycmvat2Tocmvat") // TODO: Rename puatg when available
  puund puund[] // TODO: Rename puund when available
  skpin skpin[] // TODO: Rename skpin when available
  skund skund[] // TODO: Rename skund when available

  @@index([unitId], map: "fkcmvatcmuni")
  @@index([name], map: "incmvat1")
}