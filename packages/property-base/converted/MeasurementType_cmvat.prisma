  systemStandard Int @default(0, map: "DF__cmvat__repabkod__21836D8A") @map("repabkod") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Evaluate the usage of timestamp later

  avark avark[] // TODO: Rename avark when available
  cmart_cmart_keycmvatTocmvat cmart[] @relation("cmart_keycmvatTocmvat") // TODO: Rename cmart when available
