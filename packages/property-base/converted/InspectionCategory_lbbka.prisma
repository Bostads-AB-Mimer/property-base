model InspectionCategory {
  @@map("lbbka")

  inspectionCategoryId String @id(map: "pklbbka") @map("keylbbka") @db.Char(15)  
  inspectionProtocolId String? @map("keylbpro") @db.Char(15) // TODO: Change to new model name when available
  nextInspectionCategoryId String? @map("keylbbka2") @db.Char(15)
  resourcePlanningTypeId String? @map("keybaret") @db.Char(15) // TODO: Change to new model name when available
  formId String? @map("keybsdef") @db.Char(15)
  inspectionNotificationFormId String? @map("keybsdef2") @db.Char(15)
  upcomingTenantFormId String? @map("keybsdef3") @db.Char(15)
  categoryCode String @unique(map: "aklbbkacode") @map("code") @db.Char(10)
  categoryName String? @map("caption") @db.VarChar(30)
  inspectionType Int @default(0, map: "DF__lbbka__bestype__6C1D3790") @map("bestype") @db.TinyInt
  sendEmail Int @default(0, map: "DF__lbbka__sendmail__6D115BC9") @map("sendmail") @db.TinyInt
  inspectionInterval Int @default(0, map: "DF__lbbka__besdur__6E058002") @map("besdur") @db.SmallInt
  isSent Int @default(0, map: "DF__lbbka__isent__6EF9A43B") @map("isent") @db.TinyInt
  isPublished Int @default(1, map: "DF__lbbka__publish__6FEDC874") @map("publish") @db.TinyInt
  mobileSync Int @default(1, map: "DF__lbbka__mobilesyn__70E1ECAD") @map("mobilesync") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: consider changing timestamp later when knowing more about its usage

  apartmentInspections ApartmentInspection[] @relation("lbbkaTolbbes")
  resourcePlanningType baret? @relation(fields: [resourcePlanningTypeId], references: [keybaret], onDelete: NoAction, onUpdate: NoAction, map: "fklbbkakeybaret")
  formTemplate bsdef? @relation("lbbka_keybsdefTobsdef", fields: [formId], references: [keybsdef], onDelete: NoAction, onUpdate: NoAction, map: "fklbbkakeybsdef")
  inspectionNotificationForm bsdef? @relation("lbbka_keybsdef2Tobsdef", fields: [inspectionNotificationFormId], references: [keybsdef], onDelete: NoAction, onUpdate: NoAction, map: "fklbbkakeybsdef2")
  upcomingTenantForm bsdef? @relation("lbbka_keybsdef3Tobsdef", fields: [upcomingTenantFormId], references: [keybsdef], onDelete: NoAction, onUpdate: NoAction, map: "fklbbkakeybsdef3")
  nextInspectionCategory InspectionCategory? @relation("lbbkaTolbbka", fields: [nextInspectionCategoryId], references: [inspectionCategoryId], onDelete: NoAction, onUpdate: NoAction, map: "fklbbkakeylbbka2")
  previousInspectionCategories InspectionCategory[] @relation("lbbkaTolbbka")
  inspectionProtocol lbpro? @relation(fields: [inspectionProtocolId], references: [keylbpro], onDelete: NoAction, onUpdate: NoAction, map: "fklbbkakeylbpro")

  @@index([resourcePlanningTypeId], map: "fklbbkabaret")
  @@index([formId], map: "fklbbkabsdef")
  @@index([inspectionNotificationFormId], map: "fklbbkabsdef2")
  @@index([upcomingTenantFormId], map: "fklbbkabsdef3")
  @@index([nextInspectionCategoryId], map: "fklbbkalbbka2")
  @@index([inspectionProtocolId], map: "fklbbkalbpro")
}