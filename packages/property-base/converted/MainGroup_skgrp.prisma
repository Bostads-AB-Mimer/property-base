model MainGroup {
  @@map("skgrp")

  id String @id(map: "pkskgrp") @map("keyskgrp") @db.Char(15)
  planningTypeId String @map("keybaret") @db.Char(15)
  name String @unique(map: "akskgrp") @map("caption") @db.VarChar(30)
  useInCleaning Int @default(0, map: "DF__skgrp__stundused__01117E4E") @map("stundused") @db.TinyInt
  systemStandard Int @default(0, map: "DF__skgrp__repabkod__0205A287") @map("repabkod") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: review usage context to determine if a name change is required 

  maintenanceActions MaintenanceAction[] @relation("fkskatgkeyskgrp") // TODO: ensured consistency with the renamed MaintenanceAction model
  baret baret @relation(fields: [planningTypeId], references: [keybaret], onUpdate: NoAction, map: "fkskgrpkeybaret") // TODO: change baret to new model name when available
  periodicWorks PeriodicWork[] @relation("fkskundkeyskgrp") // TODO: ensured consistency with the renamed PeriodicWork model

  @@index([planningTypeId], map: "fkskgrpbaret")
}