model LettingRound {
  @@map("bkero")
  
  lettingRoundId String @id(map: "pkbkero") @map("keybkero") @db.Char(15)
  objectId String @map("keycmobj") @db.Char(15)
  lettingRoundCode String @map("bkerokod") @db.Char(30)
  terminatingContractId String? @map("keyhyobj") @db.Char(15)
  offerDate DateTime @map("erbjdatum") @db.DateTime
  latestResponseDate DateTime @map("senastsvar") @db.DateTime
  originalLastReplyDate DateTime? @map("orglastans") @db.DateTime
  status Int @map("avslutad") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when usage is determined
  
  contract Contract? @relation(fields: [terminatingContractId], references: [contractId], onUpdate: NoAction, map: "fkbkerbkeyhyobj")
  
  // TODO: keyhyuto is not defined, leave as is for backward pass
  @@index([objectId], map: "fkbkerocmobj")
  @@index([terminatingContractId], map: "fkbkerohyobj")
  // @@index([keyhyuto], map: "fkbkerohyuto") // TODO: keyhyuto is not defined, comment out for now
}