model Placement {
  @@map("aopla")
  
  // Required fields
  id String @id(map: "pkaopla") @map("keyaopla") @db.Char(15)
  code String @unique(map: "akaopla1") @map("code") @db.VarChar(10)
  
  // Optional fields
  name String? @map("caption") @db.VarChar(70)
  isRelatedToApartment Int @default(0, map: "DF__aopla__lbactive__348CC248") @map("lbactive") @db.TinyInt
  inspectionControlDevice int @default(1, map: "DF__aopla__lbcontrol__3580E681") @map("lbcontrol") @db.TinyInt
  activeInAO int @default(1, map: "DF__aopla__useao__36750ABA") @map("useao") @db.TinyInt
  activeInLB int @default(1, map: "DF__aopla__uselb__37692EF3") @map("uselb") @db.TinyInt
  isPublished Int @default(0, map: "DF__aopla__publish__385D532C") @map("publish") @db.TinyInt
  isActive Int @default(1, map: "DF__aopla__isactive__39517765") @map("isactive") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10)
  
  // Relations
  aopar aopar[] 
  aopbd aopbd[]
  aoplo aoplo[]
  aosta aosta[] 
  aoupr aoupr[]
  barum barum[]
  lbanm lbanm[] 
  lbpla lbpla[] 
}