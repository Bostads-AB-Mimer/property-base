model LockCylinder {
  @@map("nycyl")
  
  id String @id(map: "pknycyl") @map("keynycyl") @db.Char(15)
  placementId String? @map("keycmobj") @db.Char(15) // cmobj: Placement, TODO: Change cmobj to the new model name when available
  cylinderTypeId String @map("keynycty") @db.Char(15) // nycty: CylinderType, TODO: Change nycty to the new model name when available
  cylinderCode String @map("code") @db.VarChar(30)
  description String? @map("caption") @db.VarChar(30)
  isUsed Int @default(0, map: "DF__nycyl__anvand__6428A725") @map("anvand") @db.TinyInt
  isDiscarded Int @default(0, map: "DF__nycyl__kasserad__651CCB5E") @map("kasserad") @db.TinyInt
  isMissing Int @default(0, map: "DF__nycyl__saknad__6610EF97") @map("saknad") @db.TinyInt
  isExtraOrdered Int @default(0, map: "DF__nycyl__extrabest__670513D0") @map("extrabest") @db.TinyInt
  manufactureDate DateTime? @map("tillvdat") @db.DateTime
  useDate DateTime? @map("anvdat") @db.DateTime
  note String? @map("placering") @db.VarChar(60)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  placement PropertyObject? @relation(fields: [placementId], references: [propertyObjectId], onUpdate: NoAction, map: "fknycylkeycmobj ")
  nycty nycty @relation(fields: [cylinderTypeId], references: [keynycty], onUpdate: NoAction, map: "fknycylkeynycty ")

  @@index([placementId], map: "fknycylcmobj")
  @@index([cylinderTypeId], map: "fknycylnycty")
}