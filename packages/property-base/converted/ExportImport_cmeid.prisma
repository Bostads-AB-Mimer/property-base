model ExportImport {
  @@map("cmeid")

  // Fields
  exportImportId String @id(map: "pkcmeid") @map("keycmeid") @db.Char(15)
  moduleId String @map("keysymdl") @db.Char(15)
  integrationSchemaId String? @map("keycmmap") @db.Char(15)
  exportFlag Int @map("export") @db.TinyInt
  systemName String? @map("system") @db.VarChar(30)
  typeId Int @map("type") @db.SmallInt
  filePath String? @map("path") @db.VarChar(200)
  supplierName String? @map("supplier") @db.VarChar(30)
  fileName String? @map("filename") @db.VarChar(256)
  removeLeadingZeros Int @map("remzerobeg") @db.TinyInt
  removeTrailingZeros Int @map("remzeroend") @db.TinyInt
  isActive Int @map("eidused") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: Assess usage context to potentially rename in the future

  integrationSchema IntegrationSchema? @relation(fields: [integrationSchemaId], references: [integrationSchemaId], onDelete: NoAction, onUpdate: NoAction, map: "fkcmeidkeycmmap")
  invoice Invoice[] // TODO: Rename krfkh to new model name when available

  @@index([integrationSchemaId], map: "fkcmeidcmmap")
  @@index([moduleId], map: "fkcmeidsymdl")
}