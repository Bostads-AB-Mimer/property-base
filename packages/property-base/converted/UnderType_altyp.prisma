model UnderType {
  @@map("altyp")

  id String @id(map: "PK__altyp__7E8D9008") @map("keyaltyp") @db.Char(15)
  name String @map("caption") @db.Char(30)
  isSystemStandard Int @default(0, map: "DF__altyp__repabkod__7F81B441") @map("repabkod") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage

  assets Asset[] @relation("fkalobjaltyp") // updated to use the new model name Asset instead of alobj
}