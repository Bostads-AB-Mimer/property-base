model InspectionProtocol {
  @@map("fbbpk")
  
  protocolId String @id(map: "pkfbbpk") @map("keyfbbpk") @db.Char(15)
  inspectorId String @map("keycmctc") @db.Char(15)
  protocolTemplateId String @map("keyfbbla") @db.Char(15)
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  cmctc Contact @relation(fields: [inspectorId], references: [contactId], onDelete: Cascade, onUpdate: NoAction, map: "fkfbbpkkeycmctc")
  
  // TODO: Update fbbla model name when it's available
  fbbla fbbla @relation(fields: [protocolTemplateId], references: [protocolTemplateId], onDelete: Cascade, onUpdate: NoAction, map: "fkfbbpkkeyfbbla")

  @@unique([inspectorId, protocolTemplateId], map: "akfbbpk1")
  @@index([protocolTemplateId], map: "fkfbbpkfbbla")
}