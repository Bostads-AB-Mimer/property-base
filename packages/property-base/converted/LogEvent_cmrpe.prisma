model LogEvent {
  @@map("cmrpe")
  
  logEventId String @id(map: "pkcmrpe") @map("keycmrpe") @db.Char(15)
  reportId String @map("keycmrpt") @db.Char(15)
  rowDetailsId String? @map("keycmrbr") @db.Char(15)
  userId String? @map("keysyusr") @db.Char(15)
  eventTimestamp DateTime @map("rpttime") @db.DateTime
  timestamp String @map("timestamp") @db.Char(10) // TODO: change this field later when knowing about its usage 

  reportBatchRow ReportBatchRow? @relation(fields: [rowDetailsId], references: [reportBatchRowId], onUpdate: NoAction, map: "fkcmrpekeycmrbr")
  report Report @relation(fields: [reportId], references: [reportId], onDelete: Cascade, onUpdate: NoAction, map: "fkcmrpekeycmrpt")

  @@index([rowDetailsId], map: "fkcmrpecmrbr")
  @@index([reportId], map: "fkcmrpecmrpt")
  @@index([userId], map: "fkcmrpesyusr")
}