model InspectionRemark {
  @@map("lbanm")
  
  remarkId String @id(map: "pklbanm") @map("keylbanm") @db.Char(15)
  inspectionId String @map("keylbbes") @db.Char(15)
  remarkStatusId String @map("keylbrsd") @db.Char(15)
  locationId String @map("keyaopla") @db.Char(15)
  buildingPartId String @map("keyaobdl") @db.Char(15)
  detailId String? @map("keyaodet") @db.Char(15)
  articleId String? @map("keycmart") @db.Char(15)
  priceListRegisteredId String? @map("keylbplr") @db.Char(15)
  remarksNote String? @map("notering") @db.VarChar(250)
  remarkDegree Int @default(0, map: "DF__lbanm__status__7B5F7B20") @map("status") @db.TinyInt
  workOrderCreated Int @default(0, map: "DF__lbanm__aocreated__7C539F59") @map("aocreated") @db.TinyInt
  missing Int @default(0, map: "DF__lbanm__missing__7D47C392") @map("missing") @db.TinyInt
  correctedDate DateTime? @map("atgdatum") @db.DateTime
  quantity Float @default(0, map: "DF__lbanm__quantity__7E3BE7CB") @map("quantity") @db.Money
  costAmount Float @default(0, map: "DF__lbanm__kostnad__7F300C04") @map("kostnad") @db.Money
  invoiceRecipient Int @default(0, map: "DF__lbanm__increc__0024303D") @map("increc") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10)

  buildingPart BuildingPart @relation(fields: [buildingPartId], references: [keyaobdl], onUpdate: NoAction, map: "fklbanmkeyaobdl")
  detail Detail? @relation(fields: [detailId], references: [detailId], onDelete: NoAction, onUpdate: NoAction, map: "fklbanmkeyaodet")
  location aopla @relation(fields: [locationId], references: [keyaopla], onUpdate: NoAction, map: "fklbanmkeyaopla")
  article Article? @relation(fields: [articleId], references: [articleId], onDelete: NoAction, onUpdate: NoAction, map: "fklbanmkeycmart")
  inspection ApartmentInspection @relation(fields: [inspectionId], references: [keylbbes], onUpdate: NoAction, map: "fklbanmkeylbbes")
  // TODO: rename lbplr once the new model name is decided
  remarkStatus lbrsd @relation(fields: [remarkStatusId], references: [keylbrsd], onUpdate: NoAction, map: "fklbanmkeylbrsd")
  priceListRegistered lbplr? @relation(fields: [priceListRegisteredId], references: [keylbplr], onUpdate: NoAction, map: "fklbanmkeylbplr")

  @@index([buildingPartId], map: "fklbanmaobdl")
  @@index([detailId], map: "fklbanmaodet")
  @@index([locationId], map: "fklbanmaopla")
  @@index([articleId], map: "fklbanmcmart")
  @@index([inspectionId], map: "fklbanmlbbes")
  @@index([priceListRegisteredId], map: "fklbanmlbplr")
  @@index([remarkStatusId], map: "fklbanmlbrsd")
}