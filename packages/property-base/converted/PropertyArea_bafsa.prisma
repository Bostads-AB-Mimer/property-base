model PropertyArea {
  @@map("bafsa")
  propertyAreaId String @id(map: "pkbafsa") @map("keybafsa") @db.Char(15)
  objectId String @map("keycmobj") @db.Char(15)
  areaDate DateTime @map("fdate") @db.DateTime
  areaAmount Decimal @map("area") @db.Decimal(7, 1)
  timestamp String @map("timestamp") @db.Char(10) // TODO: reconsider the field usage depending on the context

  cmobj cmobj @relation(fields: [objectId], references: [keycmobj], onDelete: Cascade, onUpdate: NoAction, map: "fkbafsakeycmobj") // TODO: replace cmobj with the new model name when available

  @@unique([objectId, areaDate], map: "akbafsa1")
}