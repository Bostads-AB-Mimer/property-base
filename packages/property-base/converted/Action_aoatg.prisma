model Action {
  @@map("aoatg")

  actionId String @id(map: "pkaoatg") @map("keyaoatg") @db.Char(15)
  actionCode String @unique(map: "akaoatg1") @map("code") @db.VarChar(10)
  name String? @map("caption") @db.VarChar(70)
  isPublished Int @default(0, map: "DF__aoatg__publish__2179EDD4") @map("publish") @db.TinyInt
  isActive Int @default(1, map: "DF__aoatg__isactive__226E120D") @map("isactive") @db.TinyInt
  timestamp String @map("timestamp") @db.Char(10) // TODO: change timestamp later when knowing more about its usage 

  workOrderRows aopar[] // TODO: change aopar to new model name if other name is decided
  statistics aosta[] // TODO: change aosta to new model name if other name is decided
  aoupr aoupr[] // TODO: change aoupr to new model name if other name is decided
}