model Subtype {
  @@map("dosty")
  
  subtypeId String @id(map: "pkdosty") @map("keydosty") @db.Char(15)
  documentTypeId String @map("keydotyp") @db.Char(15)
  name String @map("caption") @db.VarChar(60)
  timestamp String @map("timestamp") @db.Char(10) // TODO: consider changing timestamp later when knowing more about its usage
  
  formTemplates FormTemplate[] @relation("fkbsdefdosty") // Changed bsdef to FormTemplate
  documents Document[] @relation("fkdorevdosty") // Changed dorev to Document
  
  @@unique([documentTypeId, name], map: "akdosty1")
  @@index([documentTypeId], map: "fkdostydotyp")
}