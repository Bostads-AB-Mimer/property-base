model ContractType {
  contractTypeId     String   @id @map("keyhyhav")     @db.Char(15)       // Kontraktstyp (ID)
  agreementFormId    String   @map("keyhyobt")         @db.Char(15)       // Avtalsform
  contractTypeName   String   @map("hyhavben")         @db.Char(30)       // Kontraktstyp (Name)
  incomeReportFlag   Int      @map("kontruppg")        @db.TinyInt        // Kontrolluppgift
  systemStandardCode Int      @map("repabkod")         @default(0) @db.TinyInt // Systemstandard
  timestampValue     String   @map("timestamp")        @db.Char(10)       // Timestamp

	// Relations
  Contract Contract[] @relation("Contract_contractType")

  @@index([agreementFormId], name: "fkhyhavhyobt")
  @@map("hyhav")
}

