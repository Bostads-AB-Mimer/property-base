model PropertyStructureRelation {
  id                      String  @id(map: "pkbaxyk") @map("keybaxyk") @db.Char(15)
  relationTypeId          String? @map("keybaxyt") @db.Char(15)  // related table is empty?
  propertyObjectIdFrom    String  @map("keycmobj") @db.Char(15)  // mu_babuf
  propertyObjectIdTo      String  @map("keycmobj2") @db.Char(15) // prop_babuf
  timestamp               String  @map("timestamp") @db.Char(10)

  propertyStructure1 PropertyStructure @relation("PropertyStructure1", fields: [propertyObjectIdFrom], references: [propertyObjectId], onUpdate: NoAction, onDelete: NoAction, map: "fkbaxykcmobj")
  propertyStructure2 PropertyStructure @relation("PropertyStructure2", fields: [propertyObjectIdTo], references: [propertyObjectId], map: "fkbaxykcmobj2")

  @@unique([propertyObjectIdFrom, propertyObjectIdTo], map: "akbaxyk")
  @@index([propertyObjectIdFrom], map: "fkbaxykcmobj")
  @@index([propertyObjectIdTo], map: "fkbaxykcmobj2")
  @@map("baxyk")
}
